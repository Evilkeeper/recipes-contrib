                        "    filesystem:",
                        "        path: '%kernel.project_dir%/public/media'",
                        "        url: '/media'",
                        "    media:",
                        "        admin_controller: true",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "589fde8af23fd4eeca9dd712b300d13344a7b64a"
        }
    }
