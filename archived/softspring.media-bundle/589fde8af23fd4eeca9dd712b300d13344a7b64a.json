{
    "manifests": {
        "softspring/media-bundle": {
            "manifest": {
                "bundles": {
                    "Softspring\\MediaBundle\\SfsMediaBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "config/"
                },
                "post-install-output": [
                    "  * The <fg=green>SfsMediaBundle</> is ready to use.",
